from string import punctuation
import keyword
my_string = input('')
res = my_string.count('_') #Считает нижние подчеркивания
x = ('_') #Исключение в один знак из пунктуации
#Проверка на то что бы строка не была в пунктуации.
char = True
for char in my_string:
    if not char in punctuation or char == '_':
        char = True
        continue
#Проверка на: True в проверке пунктуации, маленькие буквы,цифру в начале строке, наличие только цифр в строке
# отсутствие зарегистрированых слов, колличество подчеркиваний или строка только '_'
if (char == True and my_string.islower() and not my_string[0].isdigit()
        and not my_string.isdigit() and not my_string in keyword.kwlist and res <2 or my_string == x) :
    a = True
    print(a)
else:
    print(False)
